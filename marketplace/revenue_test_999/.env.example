# .env.example

# Directory where plugins are stored
PLUGINS_DIRECTORY=./plugins

# Logging level (e.g., DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Path to the media library
MEDIA_LIBRARY_PATH=./media

# Default path for exporting media files
EXPORT_PATH=./exports

# Maximum size of the image cache
IMAGE_CACHE_SIZE=100

# Enable or disable automatic updates (True or False)
ENABLE_AUTO_UPDATE=True

# API endpoint for external data
API_ENDPOINT=https://api.publicapis.org/entries